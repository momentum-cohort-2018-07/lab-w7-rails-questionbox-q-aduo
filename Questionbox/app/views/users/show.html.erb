<h1><%= @user.username %>'s Questions</h1>
<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Question</th>
    </tr>
  </thead>
  <tbody>
  <% @user.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.question %></td>
    </tr>
  <% end %>
  </tbody>
</table>


<%= link_to 'Back', root_path %>